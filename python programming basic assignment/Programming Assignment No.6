def recursion(n):
   if n <= 1:
       return n
   else:
       return(recursion(n-1) + recursion(n-2))

nterms = 15
if nterms <= 0:
   print("Plese enter a positive integer")
else:
   print("Fibonacci sequence:")
   for i in range(nterms):
       print(recursion(i))

